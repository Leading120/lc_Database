CREATE DEFINER=`root`@`localhost` PROCEDURE `uspGetPassword`(IN UID varchar(50), OUT PW varchar(255))
BEGIN

set PW = (select CAST(aes_decrypt(Password,'key') AS char(255)) AS Password
		  from userinfo
		  where Email = UID
)
 
;

END ;;
